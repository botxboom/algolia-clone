chSFeed,\r\n  fetchPageFeed,\r\n  searchValue,\r\n  fetchFilterFeed,\r\n  fetchMiscFeed,\r\n  filterURL\r\n}) {\r\n  console.log(filterURL )\r\n  useEffect(() => {\r\n    fetchFeed();\r\n  },[]);\r\n\r\n  return (\r\n    <Grid column={1}>\r\n      <Grid.Row className=\"search__bar\">\r\n        <Search fetchSearchFeed={fetchSFeed} />\r\n      </Grid.Row>\r\n      <Grid.Row className=\"filters__\">\r\n        <Filters\r\n          searchvalue={searchValue}\r\n          totalResults={feedData[\"nbHits\"]}\r\n          time={feedData[\"processingTimeMS\"]}\r\n        />\r\n      </Grid.Row>\r\n      <Grid.Row className=\"feed__box\">\r\n        {feedData.loading ? (\r\n          <h2>Loading ...</h2>\r\n        ) : feedData.error ? (\r\n          <h2>{fetchFeed.error}</h2>\r\n        ) : (\r\n          <div style={{ display: \"inline-grid\" }}>\r\n            {feedData &&\r\n              feedData.hits &&\r\n              feedData.hits.map((data) => {\r\n                return (\r\n                  data.title && (\r\n                    <Post\r\n                      id={data.objectID}\r\n                      title={data.title}\r\n                      url={data.url}\r\n                      points={data.points}\r\n                      author={data.author}\r\n                      createdAt={data.created_at}\r\n                      commentCount={data.num_comments}\r\n                    />\r\n                  )\r\n                );\r\n              })}\r\n          </div>\r\n        )}\r\n      </Grid.Row>\r\n      <Grid.Row>\r\n        <PageNumbers\r\n          fetchmisfeed={fetchMiscFeed}\r\n          searchvalue={searchValue}\r\n          fetchpagefeed={fetchPageFeed}\r\n          totalHits={feedData.nbHits}\r\n        />\r\n      </Grid.Row>\r\n    </Grid>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    feedData: state.feed.data,\r\n    searchValue: state.feed.searchValue,\r\n    filterURL: state.feed.filterValue,\r\n  };\r\n};\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    fetchFeed: () => dispatch(fetchFeed()),\r\n    fetchSFeed: (input, pn) => dispatch(fetchFeed(\"\", input, pn)),\r\n    fetchPageFeed: (input = \"\", pageNumber = 0) =>\r\n      dispatch(fetchFeed(null, input, pageNumber)),\r\n    fetchFilterFeed: (filterURl, input, pageNumber=0) =>\r\n      dispatch(fetchFeed(filterURl, input, pageNumber)),\r\n    fetchMiscFeed: (fu, input, pageNumber=0) =>\r\n      dispatch(fetchMiscFeed(fu, input, pageNumber)),\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Home);\r\n","/media/botx/244ECB874ECB506A/Users/kumar/Desktop/algo/src/components/MenuBar.js",[],"/media/botx/244ECB874ECB506A/Users/kumar/Desktop/algo/src/components/Feeds.js",["101"],"import React, { useEffect } from \"react\";\r\nimport { connect } from \"react-redux\";\r\nimport { fetchFeed } from \"../redux\";\r\n\r\nfunction Feed({ feedData, fetchFeed }) {\r\n  useEffect(() => {\r\n    fetchFeed();\r\n  }, []);\r\n\r\n  return feedData.loading ? (\r\n    <h2>Loading ...</h2>\r\n  ) : feedData.error ? (\r\n    <h2>{fetchFeed.error}</h2>\r\n  ) : (\r\n    <div>\r\n      <h2>Feed List</h2>\r\n      <div>\r\n        {feedData &&\r\n          feedData.hits &&\r\n          feedData.hits.map((d) => {\r\n            return <p>{d.title}</p>;\r\n          })}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    feedData: state.feed.data,\r\n  };\r\n};\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    fetchFeed: () => dispatch(fetchFeed()),\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Feed);\r\n","/media/botx/244ECB874ECB506A/Users/kumar/Desktop/algo/src/util/hooks.js",[],"/media/botx/244ECB874ECB506A/Users/kumar/Desktop/algo/src/redux/rootReducer.js",[],"/media/botx/244ECB874ECB506A/Users/kumar/Desktop/algo/src/components/Post.js",[],"/media/botx/244ECB874ECB506A/Users/k