== null ){\r\n    filterURL = filterUrl + `&query=${q}` + `&page=${pn}`;\r\n  }else{\r\n    filterURL =  `https://hn.algolia.com/api/v1/search?query=${q}&tags=(story,comment,show_hn,ask_hn)&page=${pn}`\r\n  }\r\n  return (dispatch) => {\r\n    dispatch(fetchFeedRequest);\r\n    axios\r\n      .get(filterURL)\r\n      .then((response) => {\r\n        const data = response.data;\r\n        dispatch(fetchFeedSuccess(data, q, filterURL, pn));\r\n      })\r\n      .catch((error) => {\r\n        const errorMsg = error.message;\r\n        dispatch.fetchFeedFailure(errorMsg);\r\n      });\r\n  };\r\n};\r\n","/media/botx/244ECB874ECB506A/Users/kumar/Desktop/algo/src/redux/Feed/feedActionTypes.js",[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":23,"column":5,"nodeType":"112","endLine":23,"endColumn":7,"suggestions":"113"},{"ruleId":"110","severity":1,"message":"111","line":8,"column":6,"nodeType":"112","endLine":8,"endColumn":8,"suggestions":"114"},{"ruleId":"110","severity":1,"message":"115","line":14,"column":6,"nodeType":"112","endLine":14,"endColumn":13,"suggestions":"116"},{"ruleId":"117","severity":1,"message":"118","line":23,"column":5,"nodeType":"119","messageId":"120","endLine":23,"endColumn":101},{"ruleId":"121","severity":1,"message":"122","line":32,"column":43,"nodeType":"123","messageId":"124","endLine":32,"endColumn":44},{"ruleId":"121","severity":1,"message":"122","line":55,"column":43,"nodeType":"123","messageId":"124","endLine":55,"endColumn":44},"no-native-reassign",["125"],"no-negated-in-lhs",["126"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchFeed'. Either include it or remove the dependency array. If 'fetchFeed' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["127"],["128"],"React Hook useEffect has a missing dependency: 'fetchSearchFeed'. Either include it or remov